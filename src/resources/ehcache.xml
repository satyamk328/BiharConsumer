<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="false"
	monitoring="autodetect" dynamicConfig="true">

	<diskStore path="java.io.tmpdir" />
	
    <cache name="userDetails.userId" maxEntriesLocalHeap="1000"
		maxEntriesLocalDisk="25" eternal="false" 
		timeToIdleSeconds="600" timeToLiveSeconds="1440"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off">
		<persistence strategy="localTempSwap" />
	</cache>
	
	<cache name="userAllData" maxEntriesLocalHeap="1000"
		maxEntriesLocalDisk="25" eternal="false" 
		timeToIdleSeconds="600" timeToLiveSeconds="1440"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off">
		<persistence strategy="localTempSwap" />
	</cache>
	
	<cache name="topAllCity" maxEntriesLocalHeap="1000"
		maxEntriesLocalDisk="25" eternal="false" 
		timeToIdleSeconds="600" timeToLiveSeconds="1440"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off">
		<persistence strategy="localTempSwap" />
	</cache>
	
	<cache name="cityDetails.CityName" maxEntriesLocalHeap="1000"
		maxEntriesLocalDisk="25" eternal="false" 
		timeToIdleSeconds="600" timeToLiveSeconds="1440"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off">
		<persistence strategy="localTempSwap" />
	</cache>
	
	<cache name="cityDetails.CityId" maxEntriesLocalHeap="1000"
		maxEntriesLocalDisk="25" eternal="false" 
		timeToIdleSeconds="600" timeToLiveSeconds="1440"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off">
		<persistence strategy="localTempSwap" />
	</cache>
	
	<cache name="allAmenity" maxEntriesLocalHeap="1000"
		maxEntriesLocalDisk="25" eternal="false" 
		timeToIdleSeconds="600" timeToLiveSeconds="1440"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off">
		<persistence strategy="localTempSwap" />
	</cache>
	
	<cache name="amenityDetails.Id" maxEntriesLocalHeap="1000"
		maxEntriesLocalDisk="25" eternal="false" 
		timeToIdleSeconds="600" timeToLiveSeconds="1440"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off">
		<persistence strategy="localTempSwap" />
	</cache>

	<cache name="routesDetails" maxEntriesLocalHeap="1000"
		maxEntriesLocalDisk="25" eternal="false" 
		timeToIdleSeconds="600" timeToLiveSeconds="1440"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off">
		<persistence strategy="localTempSwap" />
	</cache>
	
	<cache name="walletHistory" maxEntriesLocalHeap="1000"
		maxEntriesLocalDisk="25" eternal="false" 
		timeToIdleSeconds="600" timeToLiveSeconds="1440"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off">
		<persistence strategy="localTempSwap" />
	</cache>
	
</ehcache>